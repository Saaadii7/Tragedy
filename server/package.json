        "node": "10.16.0"
    },
    "homepage": "https://github.com/saadii7/tradegy#readme",
    "dependencies": {
        "@hapi/joi": "16.1.4",
        "awilix": "4.2.2",
        "awilix-express": "2.1.1",
        "body-parser": "^1.19.0",
        "compression": "1.7.4",
        "cookie-session": "1.3.3",
        "cors": "^2.8.5",
        "dotenv": "^8.1.0",
        "express": "^4.17.1",
        "express-jwt": "5.3.1",
        "express-rate-limit": "5.0.0",
        "express-status-monitor": "1.2.6",
